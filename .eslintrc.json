{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": 0,
        "import/no-extraneous-dependencies": 0,
        "no-console": 1,
        "no-underscore-dangle": 0,
        "consistent-return": 0
    }
}